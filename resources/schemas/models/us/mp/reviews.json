            "ItemsSuccessResponse": {
                "type": "object",
                "properties": {
                    "itemId": {
                        "type": "string",
                        "description": "Specifies the item identifier.",
                        "example": "100015964"
                    },
                    "itemDescription": {
                        "type": "string",
                        "description": "Description of the item.",
                        "example": "View-Master Discovery Kids 3D Marine Life Toy Viewfinder Reel"
                    },
                    "sku": {
                        "type": "string",
                        "description": "Specifies the item identified. This value is defined by the seller.",
                        "example": "NK-WHT2XL"
                    }
                },
                "description": "Items."
            },
            "Success": {
                "type": "object",
                "properties": {
                    "items": {
                        "type": "array",
                        "description": "Items.",
                        "items": {
                            "$ref": "#\/components\/schemas\/ItemsSuccessResponse"
                        }
                    }
                },
                "description": "Items succeeded."
            },
            "BulkItemStatusUpdateRequest": {
                "required": [
                    "items",
                    "status"
                ],
                "type": "object",
                "properties": {
                    "status": {
                        "type": "string",
                        "description": "| Attribute | Description | Data Type |\n| --- | ----------- | ------- |\n| ENROLL | To enroll items into the program | string |\n| DISENROLL | To opt out items from the program | string |",
                        "enum": [
                            "ENROLL",
                            "DISENROLL"
                        ]
                    },
                    "items": {
                        "type": "array",
                        "description": "Items",
                        "items": {
                            "$ref": "#\/components\/schemas\/ItemsRequest"
                        }
                    }
                }
            },
            "ItemsRequest": {
                "required": [
                    "itemId"
                ],
                "type": "object",
                "properties": {
                    "itemId": {
                        "type": "string",
                        "description": "Specifies the item identifier.",
                        "example": "100015964"
                    },
                    "targetReviews": {
                        "type": "integer",
                        "description": "Custom target number of reviews requested to be collected for the item. Default is 5.",
                        "format": "int32",
                        "example": 15
                    }
                },
                "description": "Items"
            },
            "GetIrpItemsResponse": {
                "type": "object",
                "properties": {
                    "payload": {
                        "$ref": "#\/components\/schemas\/GetIrpItemsResponsePayload"
                    },
                    "statusCode": {
                        "type": "integer",
                        "description": "Status code for the request.",
                        "format": "int32",
                        "example": 200
                    },
                    "errors": {
                        "type": "array",
                        "items": {
                            "$ref": "#\/components\/schemas\/Error"
                        }
                    }
                }
            },
            "GetIrpItemsResponsePayload": {
                "type": "object",
                "properties": {
                    "totalItems": {
                        "type": "integer",
                        "description": "The total of items found.",
                        "format": "int32",
                        "example": 2348
                    },
                    "fetchedItems": {
                        "type": "integer",
                        "description": "The total of items fetched.",
                        "format": "int32",
                        "example": 200
                    },
                    "dateRange": {
                        "type": "string",
                        "description": "Date range to filter impressions, page views and sales.",
                        "default": "7DAYS",
                        "enum": [
                            "7DAYS",
                            "14DAYS",
                            "30DAYS"
                        ]
                    },
                    "items": {
                        "type": "array",
                        "items": {
                            "$ref": "#\/components\/schemas\/ItemsPayload"
                        }
                    },
                    "scrollId": {
                        "type": "string",
                        "description": "Specifies the searchId to fetch the subsequent set of results.",
                        "example": "DXF1ZXJ5QW5kRmV0Y2gBAAAAAACUyUsWamJiNUxwcDJUU0dxbHBObmJvRUF2UQ=="
                    }
                },
                "description": "Response payload."
            },
            "ItemsPayload": {
                "type": "object",
                "properties": {
                    "itemId": {
                        "type": "string",
                        "description": "Specifies the item identifier.",
                        "example": "100015964"
                    },
                    "itemName": {
                        "type": "string",
                        "description": "Name of the item.",
                        "example": "View-Master Discovery Kids 3D Marine Life Toy Viewfinder Reel"
                    },
                    "sales": {
                        "type": "integer",
                        "description": "Units sold during the specified date range. Defaults range is for last 7 days.",
                        "format": "int32",
                        "example": 45
                    },
                    "currentReviews": {
                        "type": "integer",
                        "description": "Current number of reviews on the item.",
                        "format": "int32",
                        "example": 0
                    },
                    "impressions": {
                        "type": "integer",
                        "description": "The number of times item showed up in search results in the requested date range. Defaults to last 7 days.",
                        "format": "int32",
                        "example": 1937
                    },
                    "currentPageViews": {
                        "type": "integer",
                        "description": "The number of views on the item page in the requested date range. Defaults to last 7 days.",
                        "format": "int32",
                        "example": 900
                    },
                    "reviewsCollected": {
                        "type": "integer",
                        "description": "The number of reviews collected for the item through IRP.",
                        "format": "int32",
                        "example": 0
                    },
                    "sku": {
                        "type": "string",
                        "description": "Specifies the item identified. This value is defined by the seller.",
                        "example": "NK-WHT2XL"
                    },
                    "itemStatus": {
                        "type": "string",
                        "description": "Current status of the item in IRP.",
                        "enum": [
                            "COMPLETE",
                            "ELIGIBLE",
                            "ENROLLED"
                        ]
                    },
                    "itemPriority": {
                        "type": "integer",
                        "description": "Priority of the item as recommended by Walmart.",
                        "format": "int32",
                        "example": 1
                    }
                }
            },
            "FilterRequest": {
                "required": [
                    "itemStatus"
                ],
                "type": "object",
                "properties": {
                    "itemStatus": {
                        "type": "string",
                        "description": "| Attribute | Description | Data Type |\n| --- | ----------- | ------- |\n| ELIGIBLE | To get items which are eligible for the program | string |\n| ENROLLED | To get items which are currently enrolled into the program | string |\n| COMPLETE | To get items which have attained target reviews through the program | string |",
                        "enum": [
                            "ELIGIBLE",
                            "ENROLLED",
                            "COMPLETE"
                        ]
                    },
                    "dateRange": {
                        "type": "string",
                        "description": "Date range to filter impressions, page views and sales.",
