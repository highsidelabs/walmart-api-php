                        "enum": [
                            "7DAYS",
                            "14DAYS",
                            "30DAYS"
                        ]
                    },
                    "category": {
                        "type": "string",
                        "description": "Category of the items.",
                        "example": "HARDLINES"
                    },
                    "price": {
                        "$ref": "#\/components\/schemas\/PriceRequest"
                    }
                },
                "description": "Filter request."
            },
            "GetIrpItemsRequest": {
                "required": [
                    "filter"
                ],
                "type": "object",
                "properties": {
                    "search": {
                        "type": "array",
                        "description": "Search request.",
                        "items": {
                            "$ref": "#\/components\/schemas\/SearchRequest"
                        }
                    },
                    "filter": {
                        "$ref": "#\/components\/schemas\/FilterRequest"
                    },
                    "sort": {
                        "type": "array",
                        "description": "Sort request.",
                        "items": {
                            "$ref": "#\/components\/schemas\/SortRequest"
                        }
                    }
                }
            },
            "PriceRequest": {
                "type": "object",
                "properties": {
                    "minimum": {
                        "type": "number",
                        "description": "Minimum price of the item.",
                        "format": "double",
                        "example": 10
                    },
                    "maximum": {
                        "type": "number",
                        "description": "Maximum price of the item.",
                        "format": "double",
                        "example": 1000
                    }
                },
                "description": "Price range of the items."
            },
            "SearchRequest": {
                "type": "object",
                "properties": {
                    "field": {
                        "type": "string",
                        "description": "| Attribute | Description | Data Type |\n| --- | ----------- | ------- |\n| ITEM_NAME | Product name | string |\n| SKU | Specifies the item identified. This value is defined by the seller. | string |",
                        "enum": [
                            "ITEM_NAME",
                            "SKU"
                        ]
                    },
                    "value": {
                        "type": "string"
                    }
                },
                "description": "Search request."
            },
            "SortRequest": {
                "type": "object",
                "properties": {
                    "field": {
                        "type": "string",
                        "description": "| Attribute | Description | Data Type |\n| --- | ----------- | ------- |\n| ITEM_PRIORITY | Recommended priority of the item by Walmart. | integer |\n| PRICE | Item price | integer |",
                        "enum": [
                            "ITEM_PRIORITY",
                            "PRICE"
                        ]
                    },
                    "order": {
                        "type": "string",
                        "enum": [
                            "ASC",
                            "DESC"
                        ]
                    },
                    "priority": {
                        "type": "integer",
                        "description": "Sort priority of the specified field.",
                        "format": "int32",
                        "example": 1
                    }
                },
                "description": "Sort request."
            },
            "GetCategoriesResponse": {
                "type": "object",
                "properties": {
                    "payload": {
                        "$ref": "#\/components\/schemas\/GetCategoriesResponsePayload"
                    },
                    "statusCode": {
                        "type": "integer",
                        "description": "Status code for the request.",
                        "format": "int32",
